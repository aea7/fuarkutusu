# Example environment variables
# Copy this file to .env.local for local development

# Keystatic CMS Configuration
# Always uses GitHub mode (both development and production)

# Keystatic GitHub OAuth (auto-generated on first setup - leave blank initially)
KEYSTATIC_GITHUB_CLIENT_ID=
KEYSTATIC_GITHUB_CLIENT_SECRET=
KEYSTATIC_SECRET=
NEXT_PUBLIC_KEYSTATIC_GITHUB_APP_SLUG=

# GitHub Repository Info (update with your repo details)
NEXT_PUBLIC_GITHUB_REPO_OWNER=your-github-username
NEXT_PUBLIC_GITHUB_REPO_NAME=your-repo-name
